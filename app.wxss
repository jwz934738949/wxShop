/*导入小图标为整个项目使用*/
@import "./styles/iconfont.wxss";

/*设置全局默认样式，但是无法使用通配符(*) */
page, view, text, swiper, swiper-item, image, navigator {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}


page {
    /*定义主题颜色，使用变量来实现*/
    --themeColor: #eb4450;
    /*定义统一字体大小，默认使用rpx。1px = 2rpx*/
    font-size: 28rpx;
}

image {
  width: 100%;
}